{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "cors": {
            "matchCondition": {
                "route": "/api/Todo",
                "methods": [
                    "GET",
                    "POST",
                    "OPTIONS"
                ]
            },
            "responseOverrides": {
                "response.headers.Access-Control-Allow-Origin": "*",
                "response.headers.Access-Control-Allow-Headers": "*"
            }
        }
    }
}